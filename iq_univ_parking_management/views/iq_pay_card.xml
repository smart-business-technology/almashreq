<odoo>

	<record id="iq_pay_card_form" model="ir.ui.view">
					<field name="name">iq.park.pay.form</field>
					<field name="model">iq.pay.card</field>
					<field name="arch" type="xml">
					
						<form string="Parking Payment" >
						
						<group>
					<group>

						<field name="iq_partner" />
						<field name="iq_journal" />



					</group>
					<group>

						<field name="iq_card_id" />

						<field name="iq_amount" />

					</group>
					
				</group>
				
				
						
							
							<footer>
					<button name="generate_pay" string="Confirm"
						type="object" class="btn-primary" />
					<button special="cancel" string="Cancel" class="btn-default" />
				</footer>
							
						</form>
					</field>
				</record>
				
				

</odoo>